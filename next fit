def next_fit(blocks, procs):
    b=blocks[:];alloc=[-1]*len(procs);pos=0;n=len(b)
    for i,p in enumerate(procs):
        tried=0
        while tried<n:
            j=pos%n
            if b[j]>=p:
                alloc[i]=j; b[j]-=p; pos=j; break
            pos+=1; tried+=1
        pos=(pos+1)%n
    return alloc

blocks=[100,500,200,300,600]
procs=[212,417,112,426]
a=next_fit(blocks,procs)
print(a)
